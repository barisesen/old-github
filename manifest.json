{
  "manifest_version": 2,
  "name": "Github",
  "description": "Github",
  "version": "1.0",
  "icons": {
    "48": "icon.png"
  },
  
  "permissions": ["tabs", "https://*.github.com/*"],
  "browser_action": {
     "default_icon": "icon.png"
   },
   "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "css": ["style.css"]
    }
  ]
}
